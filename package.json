{
  "name": "frontend",
  "version": "0.0.0",
  "browserslist": ["since 2018 and not dead, node 16"],
  "scripts": {
    "dev": "node server.mjs",
    "start": "node server.mjs",
    "build:client": "vite build --ssrManifest --outDir dist/client",
    "build:server": "vite build --ssr src/entry-server.js --outDir dist/server",
    "build": " cross-env THEME=theme-1 npm run build:client && cross-env THEME=theme-1  npm run build:server",
    "preview": "vite preview --port 4173",
    "test:unit": "vitest --environment jsdom",
    "test:e2e": "start-server-and-test preview http://localhost:4173/ 'cypress open --e2e'",
    "test:e2e:ci": "start-server-and-test preview http://localhost:4173/ 'cypress run --e2e'",
    "lint:fix": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs  --ignore-path .gitignore",
    "commit": "git-cz"
  },
  "dependencies": {
    "@vueuse/head": "^0.7.9",
    "ant-design-vue": "^3.2.11",
    "cookie-parser": "^1.4.6",
    "js-cookie": "^3.0.1",
    "lru-cache": "^7.14.0",
    "naive-ui": "^2.33.0",
    "qrcode": "^1.5.1",
    "uuid": "^8.3.2",
    "vant": "^3.6.1",
    "vue": "^3.2.37",
    "vue-router": "^4.1.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.0.1",
    "@commitlint/config-conventional": "^12.0.1",
    "@rushstack/eslint-patch": "^1.1.4",
    "@vitejs/plugin-vue": "^3.0.1",
    "@vitejs/plugin-vue-jsx": "^2.0.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/test-utils": "^2.0.2",
    "autoprefixer": "^10.2.5",
    "commitizen": "^4.2.3",
    "compression": "^1.7.4",
    "consola": "^2.15.3",
    "cross-env": "^7.0.3",
    "cypress": "^10.4.0",
    "eslint": "^8.21.0",
    "eslint-plugin-compat": "^4.0.2",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-vue": "^9.3.0",
    "express": "^4.18.1",
    "http-proxy-middleware": "^2.0.6",
    "jsdom": "^20.0.0",
    "less": "^4.1.3",
    "node-fetch": "^3.2.10",
    "prettier": "^2.7.1",
    "sass": "^1.54.5",
    "serve-static": "^1.15.0",
    "start-server-and-test": "^1.14.0",
    "vite": "^3.0.4",
    "vite-plugin-style-import": "^2.0.0",
    "vitest": "^0.21.0"
  }
}
